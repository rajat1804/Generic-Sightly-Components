<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Glossary Term Dialog">
                <items jcr:primaryType="cq:WidgetCollection">
                    <multi
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Add Glossary Terms"
                        name="./questionMap"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            layout="form"
                            xtype="customMultiPanel">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <glossaryterm
                                    jcr:primaryType="cq:Widget"
                                    dName="glossaryterm"
                                    fieldLabel="Enter the Glossary Term:"
                                    xtype="textfield"/>
                            </items>
                        </fieldConfig>
                    </multi>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
